<project
    xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
    xmlns = "http://maven.apache.org/POM/4.0.0"
    xsi:schemaLocation = "http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>com.telenav</groupId>
        <artifactId>telenav-superpom-project-family</artifactId>
        <version>2.0.3</version>
        <relativePath>../telenav-superpom-project-family/pom.xml</relativePath>
    </parent>
    <artifactId>telenav-superpom-intermediate-bom</artifactId>
    <version>2.0.2</version>

    <packaging>pom</packaging>
    <name>telenav-superpom-intermediate-bom</name>
    <description>
        Parent pom _only_ for use in intermediate bill-of-materials POMs that sit below
        project family checkout roots.  Disables javadoc aggregation for these intermediate
        POMs which do not need it, and for which it will fail.
    </description>

    <properties>
        <maven.javadoc.skip>true</maven.javadoc.skip>
    </properties>

</project>
